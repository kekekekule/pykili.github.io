            <span class="s">'–ø–µ—Ä—Å–æ–Ω–∞–∂1'</span><span class="p">:</span> <span class="p">[</span>
                <span class="s">'—Ä–µ–ø–ª–∏–∫–∞1'</span><span class="p">,</span>
                <span class="s">'—Ä–µ–ø–ª–∏–∫–∞2'</span><span class="p">,</span>
